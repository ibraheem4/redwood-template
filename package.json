{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ngneat/falso": "^6.4.0",
    "@redwoodjs/cli-storybook": "7.1.3",
    "@redwoodjs/core": "7.1.3",
    "@redwoodjs/realtime": "7.1.3",
    "@redwoodjs/studio": "11",
    "@redwoodjs/vite": "7.1.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.2",
    "prettier-plugin-tailwindcss": "0.4.1",
    "prisma": "latest",
    "storybook-tailwind-dark-mode": "1.0.15"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=20.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.5.1",
  "lint-staged": {
    "(web|api)/src/**/*.{js,jsx,ts,tsx}": "yarn rw lint --fix"
  },
  "scripts": {
    "format": "prettier --write '**/*.{html,js,ts,jsx,tsx,css,md,json}'",
    "prepare": "husky install",
    "storybook": "IS_STORYBOOK=true yarn rw storybook"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "typescript": "4.3.5"
  }
}
